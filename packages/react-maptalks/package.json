{
  "name": "rct-maptalks",
  "author": "Miguel Angel Ramirez (miguelonCoder)",
  "license": "Hippocratic-2.1",
  "version": "1.0.1",
  "description": "A way to use Maptalks as React Components",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "url": "https://github.com/miguelonCoder/react-maptalks",
    "type": "git"
  },
  "homepage": "https://migueloncoder.github.io/react-maptalks-doc/",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/maptalks": "^0.49.1",
    "@types/react": "^18.0.33",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "maptalks": "^1.0.0-rc.18",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  },
  "keywords": [
    "react",
    "JSX",
    "TSX",
    "mapmaptalks",
    "mapComponent",
    "vectorlayer",
    "marker",
    "ts"
  ]
}